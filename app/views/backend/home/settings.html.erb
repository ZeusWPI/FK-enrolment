<%= render :partial => 'club' %>

<%= semantic_form_for @club, :url => backend_settings_path  do |f| %>
  <%= f.inputs do %>
    <%= f.input :uses_isic %> <br \>
    <%= f.input :registration_method, :as => :select,
      :collection => {"Geen" => "none", "API" => "api", "Website" => "website"} %>

  <% end %>
  <%= f.buttons do %>
    <%= f.commit_button "Bewaar" %>
  <% end %>
<% end %>
