<%= render :partial => 'club' %>

<%= form_tag backend_search_path, :id => 'search-form', :remote => true do  %>
  <%= select_tag 'search_field',
        options_for_select({'UGent nummer' => 'ugent_nr',
                            'Naam'         => 'name',
                            'Email adres'  => 'email'}) %>
  <%= text_field_tag 'search_value', params[:search_value] %>
  <%= submit_tag "Zoek", :name => nil %>
<% end %>

<div id="search-results">
</div>
